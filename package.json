{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Portfolio App",
  "scripts": {
    "build": "npm run clean:build && webpack --mode production",
    "build:dev": "npm run clean:build && webpack --mode development",
    "ci": "npm run lint && npm run typecheck && npm test",
    "clean:build": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "coverage": "npm run clean:coverage && npm run test:coverage",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext js,ts,tsx src types webpack.config.js",
    "start": "webpack-dev-server --open",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loginov-rocks/Portfolio.git"
  },
  "author": "Danila Loginov <danila@loginov.rocks> (https://loginov.rocks)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loginov-rocks/Portfolio/issues"
  },
  "homepage": "https://loginov.rocks/Portfolio",
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "firebase": "^6.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "react-redux-firebase": "^2.3.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-firestore": "^0.8.0",
    "redux-persist": "^6.0.0",
    "redux-repository": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "0.0.75",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/recompose": "^0.30.6",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.5",
    "css-loader": "^3.1.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "greenkeeper": {
    "ignore": [
      "react-redux"
    ]
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true
  }
}
